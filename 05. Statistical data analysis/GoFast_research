   "source": [
    " # каждое значение из столбца duration округляется с помощью «потолка». До агрегации, что \n",
    "df['duration'] = np.ceil(df['duration']) \n",
    "\n",
    "                           # столбцы, по которым группируем данные\n",
    "ag_ride = (df.pivot_table(index=['user_id','month'],\n",
    "                           # значения, по которым мы хотим увидеть сводную таблицу\n",
    "                           values=['city', 'age', 'subscription_type', \n",
    "                                   'distance', 'duration', 'date',\n",
    "                                   'minute_price','start_ride_price','subscription_fee'],\n",
    "                            # функция, которая будет применяться к значениям.\n",
    "                            aggfunc={'city': 'first', \n",
    "                                     'age': 'first',\n",
    "                                     'subscription_type': 'first',\n",
    "                                     'distance': 'sum',\n",
    "                                     'duration': 'sum',\n",
    "                                     'date': 'count',\n",
    "                                     'minute_price': 'first',\n",
    "                                     'start_ride_price': 'first',\n",
    "                                     'start_ride_price': 'first',\n",
    "                                     'subscription_fee': 'first'}\n",
    "                            )\n",
    "                 # переименнуем столбцы\n",
    "                .rename(columns={'distance': 'sum_distance',\n",
    "                                 'date': 'cnt_ride',\n",
    "                                 'duration': 'sum_duration'}\n",
    "                       )\n",
    "             )\n",
    "ag_ride.head()"
   ]
