    "df = (users.merge(rides, on='user_id').merge(subscriptions, on='subscription_type'))\n",
    "print('Кол-во явных дубликатов:', df.duplicated().sum(), '\\n')\n",
    "print('Кол-во уникальных пользователей:', df['user_id'].nunique(), '\\n')\n",
    "print(df.info())"
   ]
