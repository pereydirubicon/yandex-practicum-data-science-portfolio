   "outputs": [],
   "source": [
    "# БЕЗ подписки\n",
    "df_free = df[df['subscription_type'] == 'free'] \n",
    "# С подпиской\n",
    "df_ultra = df[df['subscription_type'] == 'ultra']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "9aadb8c2",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style scoped>\n",
       "    .dataframe tbody tr th:only-of-type {\n",
       "        vertical-align: middle;\n",
       "    }\n",
       "\n",
