       "</table>\n",
       "</div>"
      ],
      "text/plain": [
       "     Тип  Пользователей  Процент\n",
       "0   free            835     54.4\n",
       "1  ultra            699     45.6"
      ]
     },
     "execution_count": 16,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Сгруппируем пользователей по подпискам и визуализируем\n",
    "(\n",
    " users.pivot_table(index='subscription_type', \n",
    "                   values='user_id', \n",
    "                   aggfunc='count')\n",
    "      \n",
    "      .plot.pie(y='user_id', \n",
    "                figsize=(8,8),  \n",
    "                label='Кол-во пользователей', \n",
    "                autopct='%1.1f%%', \n",
    "                startangle=80,\n",
    "                title='FREE vs ULTRA')\n",
    ")\n",
    "plt.show()\n",
    "\n",
    "# Создадим отдельную таблицу с типом подписки, кол-вом пользователей и процентом\n",
    "users_type = (\n",
    "               users.groupby('subscription_type')['user_id']\n",
    "                    .count().reset_index()\n",
    "                    .rename(columns = {'subscription_type': 'Тип', \n",
    "                                       'user_id': 'Пользователей'})\n",
    "              )\n",
    "users_type['Процент'] = ((users_type['Пользователей']/users_total)*100).round(1)\n",
    "users_type.sort_values(by='Процент', ascending=False)"
   ]
