   "source": [
    "# Создадим отдельную таблицу выручкой по месяцам в зависимости от типа подписки\n",
    "ag_ride_pivot = ag_ride.pivot_table(index='month',\n",
    "                                     columns='subscription_type',\n",
    "                                     values='month_revenue', \n",
    "                                     aggfunc='sum')\n",
    "# Визуализируем данные о динамике выручки по месяцам в зависимости от типа подписки                                     \n",
    "ag_ride_pivot.plot(\n",
    "                        title = 'Динамика выручки по месяцам в зависимости от типа подписки',\n",
    "                        y = ['free', 'ultra'],\n",
    "                        kind = 'line', \n",
    "                        style='o-', \n",
    "                        grid = True, figsize=(15, 5)\n",
    "                      )\n",
    "                  \n",
    "plt.xlabel('Месяц')\n",
    "plt.ylabel('Выручка, руб')\n",
    "plt.legend(['free', 'ultra'])\n",
    "plt.show()\n",
    "\n",
    "ag_ride_pivot"
   ]
