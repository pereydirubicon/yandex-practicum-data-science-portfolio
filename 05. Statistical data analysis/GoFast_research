   "source": [
    "#Создадим две выборки пользователей с данным то выручке по месяцам в зависимости от типа подписки\n",
    "month_revenue_ultra =  ag_ride[ag_ride['subscription_type'] == 'ultra']\n",
    "month_revenue_free =  ag_ride[ag_ride['subscription_type'] == 'free'] \n",
    "\n",
    "#Проверим гипотезу о том, что помесячная выручка от пользователей с подпиской выше,\n",
    "# чем выручка от пользователей без подписки\n",
    "\n",
    "# задаём уровень статистической значимости\n",
    "alpha = 0.05\n",
    "\n",
    "# проводим ttest для двух выборок\n",
    "results = st.ttest_ind(month_revenue_ultra['month_revenue'], month_revenue_free['month_revenue'], \n",
    "                       alternative='greater',# в качестве альтернативной используем одностороннюю гипотезу «больше»\n",
    "                       equal_var = False)#так как выборки у вас разного размера и дисперсии у совокупностей разные\n",
    "\n",
    "print(f'p-value: {results.pvalue}') #если p-value окажется меньше него, отвергнем нулевую гипотезу\n",
    "print('Среднее значение выручки utra:', month_revenue_ultra['month_revenue'].mean().round())\n",
    "print('Среднее значение выручки free:', month_revenue_free['month_revenue'].mean().round())\n",
    "\n",
    "# проверяем p-value\n",
    "if results.pvalue < alpha:\n",
    "    print('Отвергаем нулевую гипотезу')\n",
    "else:\n",
    "    print('Нет оснований отвергнуть нулевую гипотезу')"
   ]
