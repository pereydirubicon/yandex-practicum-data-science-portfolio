   "source": [
    "# Сгруппируем пользователей по возрастам и визуализируем\n",
    "data = (\n",
    "        users.pivot_table(index='age', \n",
    "                          values='user_id', \n",
    "                          aggfunc='count')\n",
    "             .plot(kind='bar', \n",
    "                   figsize=(15, 5))      \n",
    "        )\n",
    "data.set(xlabel='Возраст')\n",
    "data.set(ylabel='Кол-во пользователей')\n",
    "plt.title('Распределение пользователей по возрасту')\n",
    "plt.show()\n",
    "\n",
    "# Создадим отдельную таблицу с возрастом, кол-вом пользователей и процентом\n",
    "users_age = (\n",
    "               users.groupby('age')['user_id']\n",
    "                    .count().reset_index()\n",
    "                    .rename(columns = {'age': 'Возраст', \n",
    "                                       'user_id': 'Пользователей'})\n",
    "              )\n",
    "users_age['Процент'] = ((users_age['Пользователей']/users_total)*100).round(1)\n",
    "users_age.sort_values(by='Процент', ascending=False).reset_index(drop=True)"
   ]
